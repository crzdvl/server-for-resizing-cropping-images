{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            0,
            0
        ],
        "strict": 0,
        "no-var": "error",
        "no-console": 1,
        "no-fallthrough": 0,
        "consistent-return": 0,
        "max-len": 0,
        "global-require": 0,
        "no-unused-vars": 0
    }
}
